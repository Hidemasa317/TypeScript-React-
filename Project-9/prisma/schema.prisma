generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        BigInt   @id @default(autoincrement())
  name      String
  email     String   @unique
  passwordHash  String
  role      String   @default("sales_person")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


